{
 "version": 2,

 "routes": [
  {
   "src": "/(.*)",
   "dest": "index.js",
   "methods": ["GET", "PUT", "PATCH", "DELETE", "UPDATE", "POST", "OPTIONS"]
  }
 ]

 "builds": [
  {
   "src": "index.js",
      "use": "@vercel/node",
      "config": 
      {
        "includeFiles": ["package.json"]
      }
  }
 ]
}
